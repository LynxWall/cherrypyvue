{
    "name": "cherrypyvue",
    "version": "1.0.0",
    "description": "Initial CherryPy with Vue Project",
    "main": "index.js",
    "author": "Lonnie Wall",
    "license": "MIT",
    "private": true,
    "scripts": {
        "setup": "pip install -r requirements.txt && yarn",
        "startDev": "honcho start -f ./bin/Procfile.dev",
        "startVue": "webpack-dev-server --open",
        "startProd": "honcho start -f ./bin/Procfile.prod",
        "buildDev": "webpack -d --config ./webpack.config.js",
        "buildProd": "webpack -p --config ./webpack.prod.config.js",
        "lint": "yarn run eslint & yarn run pythonlint & yarn run scsslint",
        "eslint": "./node_modules/.bin/eslint src",
        "pythonlint": "flake8 --exclude=node_modules,.svn,CVS,.bzr,.hg,.git,__pycache__,.tox ./",
        "scsslint": "./node_modules/.bin/sass-lint src '**/*.scss' -v -q"
    },
    "dependencies": {
        "vee-validate": "^2.1.3",
        "vue": "^2.5.17",
        "vue-router": "^3.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.1.6",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/preset-env": "^7.1.6",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.4",
        "clean-webpack-plugin": "^1.0.0",
        "css-loader": "^1.0.1",
        "eslint": "^5.9.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-friendly-formatter": "^4.0.1",
        "eslint-loader": "^2.1.1",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-node": "^8.0.0",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-standard": "^4.0.0",
        "eslint-plugin-vue": "^5.0.0-beta.5",
        "file-loader": "^2.0.0",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "html-webpack-template": "^6.2.0",
        "node-sass": "^4.10.0",
        "node-static": "^0.7.11",
        "sass-lint": "^1.12.1",
        "sass-loader": "^7.1.0",
        "url-loader": "^1.1.2",
        "vue-hot-reload-api": "^2.3.1",
        "vue-html-loader": "^1.2.4",
        "vue-loader": "^15.4.2",
        "vue-template-compiler": "^2.5.17",
        "webpack": "^4.26.0",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.10"
    }
}
